<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="60dp">

        <View
            android:id="@+id/view_custom_button"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/grey05"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/iv_custom_button"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginRight="5dp"
            app:layout_constraintEnd_toStartOf="@id/tv_custom_button"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view_custom_button"
            app:layout_constraintBottom_toBottomOf="parent"
            android:backgroundTint="@color/black" />

        <TextView
            android:id="@+id/tv_custom_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/iv_custom_button"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="편집하기" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>